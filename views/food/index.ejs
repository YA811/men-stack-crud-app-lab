<!-- views/food/index.ejs -->

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Food Index</title>
  </head>
  <body>
    <h1>All Food</h1>
    <ul>
        <% Food.forEach(food =>{ %>
      <li><a href="/food/<%= food._id%>"><%= food.name %></a></li>
    <% })  %>  
      </ul>
    <a href="/food/new">Add New Fruit</a>
  </body>
</html>